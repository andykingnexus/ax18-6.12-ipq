
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	list ipaddr '127.0.0.1/8'

config globals 'globals'
	option packet_steering '0'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'lan1'
	list ports 'lan2'
	list ports 'lan3'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ip6assign '64'
	option ip6ifaceid 'eui64'
	list ipaddr '192.168.10.1/24'
	option delegate '0'

config interface 'wan'
	option device 'wan'
	option proto 'pppoe'
	option username '13663805869'
	option password '932905'
	option ipv6 'auto'

